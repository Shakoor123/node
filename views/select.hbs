<table class="table" border="2">
  <thead>
    <tr>
        <th scope="col">id</th>
      <th scope="col">name</th>
      <th scope="col">branch</th>
    </tr>
  </thead>
  <tbody>
      {{#each result}}
    <tr>
      <td>{{this.id}}</td>
      <td>{{this.name}}</td>
      <td>{{this.branch}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>